{
  "name": "rest-on-couch",
  "version": "5.5.1",
  "description": "Interface to CouchDB that allows the control of permissions on the documents",
  "main": "src/index.js",
  "files": [
    "bin",
    "public",
    "src",
    "views"
  ],
  "bin": {
    "rest-on-couch-import": "./bin/rest-on-couch-import.js"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./test/setup/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/data/",
      "/test/homedir/",
      "/test/utils/",
      "/test/setup/"
    ]
  },
  "scripts": {
    "build": "webpack",
    "eslint": "eslint src bin test --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier-eslint bin/*.js src/**/*.js test/**/*.js --write",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "NODE_ENV=production npm run build && node bin/rest-on-couch-server.js",
    "start:devserver": "REST_ON_COUCH_HOME_DIR=test/homedir node bin/rest-on-couch-server.js --config=conf/devConfig.js",
    "test": "npm run test-jest && npm run eslint",
    "test-jest": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rest-on-couch.git"
  },
  "keywords": [
    "couch",
    "couchdb"
  ],
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rest-on-couch/issues"
  },
  "homepage": "https://github.com/cheminfo/rest-on-couch#readme",
  "dependencies": {
    "agentkeepalive": "^3.3.0",
    "chokidar": "^2.0.2",
    "co-sleep": "0.0.1",
    "commander": "^2.12.2",
    "debug": "^3.1.0",
    "delay": "^2.0.0",
    "escape-string-regexp": "^1.0.4",
    "extend": "^3.0.0",
    "fold-to-ascii": "^4.0.0",
    "fs-extra": "^5.0.0",
    "has-own": "^1.0.0",
    "if-env": "^1.0.0",
    "jquery": "^3.1.1",
    "kcors": "^2.0.0",
    "klaw": "^2.1.1",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-passport": "^4.0.1",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.3.0",
    "koa-session": "^5.5.1",
    "koa-static": "^4.0.2",
    "ldapjs": "^1.0.0",
    "lodash": "^4.17.1",
    "md5": "^2.2.1",
    "minimist": "^1.2.0",
    "nano": "^6.4.2",
    "nunjucks": "^3.1.0",
    "object-hash": "^1.2.0",
